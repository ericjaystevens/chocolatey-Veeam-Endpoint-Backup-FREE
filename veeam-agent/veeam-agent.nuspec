    <version>3.0.0.748</version>
    <title>Veeam Agent for Microsoft Windows</title>
    <authors>Veeam</authors>
    <owners>Simon Baerlocher, Eric Stevens</owners>
    <licenseUrl>https://www.veeam.com/eula.html</licenseUrl>
